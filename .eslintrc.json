{ "extends": [
    "standard",
    "standard-react"
],
  "env": {
    "browser": true,
  },
  "rules": {
    "semi": ["error","always"],
    "max-len": [2, 100, 2],
    "enerator-star-spacing": 0,
    "babel/generator-star-spacing": 1,
    "space-before-function-paren": "off",
    "keyword-spacing": "off",
    "camelcase": 0,
    "no-multi-str": "off"
  },
  "plugins": [
    "babel"
  ],,
  "globals": {
    "__DEVELOPMENT__": false,
    "__CLIENT__": false,
    "__SERVER__": false,
    "__DISABLE_SSR__": false,
    "__DEVTOOLS__": false,
    "socket": false,
    "webpackIsomorphicTools": false
  }
}
